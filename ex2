my_list = [1, 2, 3]
print("Исходный список:", my_list)

# Изменяем первый элемент
my_list[0] = 100
print("После изменения:", my_list)

# Создаем кортеж
my_tuple = (1, 2, 3)
print("Исходный кортеж:", my_tuple)

my_tuple[0] = 100
